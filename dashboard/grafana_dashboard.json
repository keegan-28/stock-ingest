          "format": "table",
          "hide": false,
          "rawQuery": true,
          "rawSql": "WITH base AS (\n    SELECT\n        ticker,\n        timestamp,\n        close,\n        LAG(close, 1) OVER (PARTITION BY ticker ORDER BY timestamp) AS prev_close_1d,\n        LAG(close, 5) OVER (PARTITION BY ticker ORDER BY timestamp) AS prev_close_1w,\n        LAG(close, 20) OVER (PARTITION BY ticker ORDER BY timestamp) AS prev_close_1m,\n        FIRST_VALUE(close) OVER (PARTITION BY ticker, date_trunc('year', timestamp) ORDER BY timestamp) AS first_close_ytd,\n        LAG(close, 252) OVER (PARTITION BY ticker ORDER BY timestamp) AS prev_close_1y\n    FROM stockticks \n),\nlatest AS (\n    SELECT\n        ticker,\n        MAX(timestamp) AS max_date\n    FROM stockticks \n    GROUP BY ticker\n)\nSELECT\n    b.ticker,\n    ROUND(((close - prev_close_1d) / prev_close_1d * 100)::numeric, 2) AS \"1 Day\",\n    ROUND(((close - prev_close_1w) / prev_close_1w * 100)::numeric, 2) AS \"1 Week\",\n    ROUND(((close - prev_close_1m) / prev_close_1m * 100)::numeric, 2) AS \"1 Month\",\n    ROUND(((close - first_close_ytd) / first_close_ytd * 100)::numeric, 2) AS \"YTD\",\n    ROUND(((close - prev_close_1y) / prev_close_1y * 100)::numeric, 2) AS \"1 Year\"\nFROM base b\nJOIN latest l\n  ON b.ticker = l.ticker AND b.timestamp = l.max_date\nORDER BY b.ticker;",
